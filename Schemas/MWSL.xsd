<?xml version="1.0" encoding="iso-8859-1"?>
<!-- Schema for Workspace-spec in Java-client -->
<!-- MOK 060606 Initial work -->
<!-- MOK 070607 Version 0.1   First release -->
<!-- MOK 070831 Version 0.2   Support for tag: Empty and default layouts added  -->
<!-- MOK 070918 Version 0.3   Empty-tag adjustments-->
<!-- MOK 071018 Version 0.4   title- and layout-attr removed from Hidden-tag -->
<!-- MOK 090301 Version 0.5   Added support for Formation- Segment and Fix-tag. Empty-tag removed. Other cleanup -->
<!-- MOK 090301 Version 0.6   Cleanup and desupport of Workspaces-tag (links to other workspaces) -->
<!-- MOK 090402 Version 0.7   Support for filters within workspaces -->
<!-- MOK 090427 Version 0.8   Attr: collapsed on clumps changed name to minimized -->
<!-- MOK 090427 Version 0.9   name-attr added for many tags, other cleanup  -->
<!--                          '%' in front of a deprecated tag/attr means that is also desupported -->
<!-- MOK 090902 Version 0.10  title-attr removed for CardTable-tag, only Hidden tags allowed in Addendums, 
                              name- and title-attr removed from Workspace-tag. 
                              ActionCard-tag must follow a Fix-tag (binding)
                              reversedForeignKey is mandatory on Restrict-tag -->
<!-- MOK 090907 Version 0.11  horizontal-attr for Formation-tag is desupported
                              %Dashboard-tag is deprecated, support for multiple components in Workspace-tag,
                              %CardTable-tag is deprecated, 
                              %Segment-tag is deprecated, replaced by Section-tag 
                              %Addendums-tag is deprecated -->
<!-- MOK 091110 Version 0.12  With-tag is introduced
                              %dialog-attr on pane-tags are deprecated, replaced by source-attr 
                              %CardTable-tag is desupported 
                              %ActionCard-tag is deprecated, replaced by Card-tag
                              %Fix-tag is deprecated, replaced by Mount-tag 
                              %UpperBind-tag is desupported -->               
<!-- MOK 100223 Version 0.13  Segment-tag is desupported
                              ActionCard-tag is desupported 
                              Fix-tag is is desupported 
                              Dashboard-tag is desupported
                              dialog-attr on pane-tags is desupported 
                              Addendums-tag is deprecated            
                              foreignKey on Bind-tag is mandatory -->
<!--  MOK 100329 Version 0.14 hidden-attr is added to panes
                              hidden-attr is added to clumps 
                              view-attr on panes can be defined empty -->
<!--  ANH 100512 Version 0.15 new Define-tag as child of Workspace-tag
                              hidden-attr is changed to expression type -->
<!--  ANH 100601 Version 0.16 changed view-attr from OptIdType to ExpressionType -->
<!--  MOK 100616 Version 0.17 parentSize-attr added to clump-tags (Expansions and Assistants)-->
<!--  MOK 100719 Version 0.18 Section-tag extended for general use. Through-tag added
                              Define-tag can only be placed as first tag in a Workspace-tag (now controlled by the xsd) 
                              name-attr on Workspace is mandatory (has always been, now also by spec.)-->
<!--  MOK 100908 Version 0.19 Workspace-tag allowed in Through-tag -->              
<!--  MOK 100910 Version 0.20 . parentSize-attr and minimized-attr is prohibited for Extensions-tags directly under a Section-tag
                                source-attr in Workspace-tag is added, replaces ref-attr -->
<!--  MOK 100910 Version 0.21 X hidden-attr do now support 'isEmpty()' (and as before 'hasNoData()') -->
<!--  MOK 2011-03-29 Ver 0.22 . hidden-attr added to section-tag -->
<!--  MOK 2011-18-08 Ver 0.23 X Namespace changed from maconomy to deltek.
                                Name for primary-fk is now 'primary' (used to be 'self'). 'with' and 'mount' are added as default names for these bindings. -->

<!-- Indication after version number: . = backward compatible, X = NOT backward compatable, conversion needed -->

<xs:schema targetNamespace="http://www.deltek.com/ns/mwsl"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
           jxb:version="2.0"
           xmlns="http://www.deltek.com/ns/mwsl"           
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           xmlns:ai="http://jaxb.dev.java.net/plugin/if_insertion"
           jxb:extensionBindingPrefixes="ai">
  <!--
	-->
  <xs:include schemaLocation="Basics.xsd" />
  <!--
  Main element: MWSL
	-->
  <xs:complexType name="XMWSLBase">
    <xs:choice>
      <xs:element name="Workspace" type="XWorkspace"/>
    </xs:choice>
    <!-- minVersion is the minimum supported version -->
    <xs:attribute name="minVersion" type="VersionType" fixed="0.23" />
    <!-- maxVersion is the version of the current xsd (=current version) -->
    <xs:attribute name="maxVersion" type="VersionType" fixed="0.23" />
  </xs:complexType>
  <!--
  -->  
  <xs:simpleType name="MWSLVersionType">
    <xs:restriction base="VersionType">
      <xs:pattern value="0.(2[3])"/>
    </xs:restriction>
  </xs:simpleType>
  <!--
  -->
  <xs:element name="MWSL" type="XMWSL"/>
  <!--
  -->
  <xs:complexType name="XMWSL">
    <xs:complexContent>
      <xs:extension base="XMWSLBase">
        <xs:attribute name="version" type="MWSLVersionType" use="required" />
        <xs:attribute name="maxVersion" use="prohibited" />
        <xs:attribute name="minVersion" use="prohibited" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>  
  <!--
  Generic dialog
	-->
  <xs:simpleType name="XeComponentType">
    <xs:restriction base="IdType">
      <xs:enumeration value="Filter"/>    
      <xs:enumeration value="Card"/>
      <xs:enumeration value="Table"/>
      <xs:enumeration value="PrintCard"/>
      <xs:enumeration value="Hidden"/>
      <xs:enumeration value="Section"/>
      <xs:enumeration value="Root"/>
      <xs:enumeration value="Reference"/> 
      <xs:enumeration value="Wizard"/> 
      <xs:enumeration value="Search"/>
    </xs:restriction>
  </xs:simpleType>
  <!--
  -->
  <xs:simpleType name="XHiddenType">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="hasNoRecords()" />
          <xs:enumeration value="hasNoChildren()" />
          <xs:enumeration value="hasNoSeed()" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="XExpressionType"/>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <!--
  -->
  <xs:simpleType name="XeClumpType">
    <xs:restriction base="IdType">
      <xs:enumeration value="Expansions"/>    
      <xs:enumeration value="Assistants"/>
      <xs:enumeration value="Formation"/>
      <xs:enumeration value="Detached"/>
    </xs:restriction>
  </xs:simpleType>
  <!--
  -->
  <xs:simpleType name="XeConnectionType">
    <xs:restriction base="IdType">
      <xs:enumeration value="Bind"/>    
      <xs:enumeration value="Restrict"/>
      <xs:enumeration value="Mount"/>
      <xs:enumeration value="Transparent"/>
      <xs:enumeration value="With"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- 
  -->
  <xs:complexType name="XComponentBase">
    <xs:group ref="subBase_g" minOccurs="0"/>
    <xs:attribute name="name" type="IdType"/>
    <xs:attribute name="title" type="TitleType"/>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XPaneBase" abstract="true">
    <xs:annotation><xs:appinfo>
        <ai:interfaces check="false">jaxb.mwsl.structure.XiComponent</ai:interfaces>
    </xs:appinfo></xs:annotation>
    <xs:complexContent>
      <xs:extension base="XComponentBase">
        <xs:attribute name="layout" type="OptIdType"/>
        <xs:attribute name="view" type="XExpressionType" />
        <xs:attribute name="source" type="IdType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XDialogComponentBase" abstract="true">
    <xs:complexContent>
      <xs:restriction base="XPaneBase">
        <xs:group ref="subBase_g" minOccurs="0"/>
        <xs:attribute name="source" type="IdType" /> 
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XVisibleDialogComponentBase" abstract="true">
    <xs:complexContent>
      <xs:extension base="XDialogComponentBase"/>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XInvisibleDialogComponentBase" abstract="true">
    <xs:complexContent>
      <xs:restriction base="XDialogComponentBase">
        <xs:group ref="subHidden_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XFilterBaseBase">
    <xs:complexContent>
      <xs:extension base="XDialogComponentBase">
        <xs:attribute name="pluginId" type="PluginIdType" default="com.maconomy.client.pane.filter"/>
        <xs:attribute name="type" type="XeComponentType" fixed="Filter"/>
        <xs:attribute name="hidden" type="XHiddenType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XFilterBase">
    <xs:complexContent>
      <xs:restriction base="XFilterBaseBase">
        <xs:group ref="subFilter_g" minOccurs="0"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XFilter">
    <xs:complexContent>
      <xs:restriction base="XFilterBase">
        <xs:group ref="subFilter_g" minOccurs="0"/>
        <xs:attribute name="source" type="IdType" use="required"/>                
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XWithFilter">
    <xs:complexContent>
      <xs:restriction base="XFilterBase">
        <xs:group ref="subFilter_g" minOccurs="0"/>
        <xs:attribute name="source" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XTableBaseBase">
    <xs:complexContent>
      <xs:extension base="XVisibleDialogComponentBase">
        <xs:attribute name="cardPluginId" type="PluginIdType" default="com.maconomy.client.pane.card"/>
        <xs:attribute name="pluginId" type="PluginIdType" default="com.maconomy.client.pane.table"/>
        <xs:attribute name="type" type="XeComponentType" fixed="Table"/>
        <xs:attribute name="hidden" type="XHiddenType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XTableBase">
    <xs:complexContent>
      <xs:restriction base="XTableBaseBase">
        <xs:group ref="subTable_g" minOccurs="0"/>
        <xs:attribute name="cardPluginId" use="prohibited"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
    <!--
  -->
  <xs:complexType name="XTable">
    <xs:complexContent>
      <xs:restriction base="XTableBase">
     	  <xs:group ref="subTable_g" minOccurs="0"/>
        <xs:attribute name="cardPluginId" use="prohibited"/>
        <xs:attribute name="type" use="prohibited"/>
        <xs:attribute name="source" type="IdType" use="required"/>                
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XWithTable">
    <xs:complexContent>
      <xs:restriction base="XTableBase">
        <xs:group ref="subTable_g" minOccurs="0"/>
        <xs:attribute name="source" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  Card (complete or no table) from Card-dialog
	-->
  <xs:complexType name="XCardBaseBase">
    <xs:complexContent>
      <xs:extension base="XVisibleDialogComponentBase">
        <xs:attribute name="pluginId" type="PluginIdType" default="com.maconomy.client.pane.card"/>
        <xs:attribute name="type" type="XeComponentType" fixed="Card"/>
        <xs:attribute name="hidden" type="XHiddenType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- 
   -->
  <xs:complexType name="XCardBase">
    <xs:complexContent>
      <xs:restriction base="XCardBaseBase">
        <xs:group ref="subSingle_g" minOccurs="0"/>
        <xs:attribute name="type" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!-- 
   -->
  <xs:complexType name="XCard">
    <xs:complexContent>
      <xs:restriction base="XCardBase">
        <xs:group ref="subSingle_g" minOccurs="0"/>
        <xs:attribute name="source" type="IdType" use="required"/> 
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!-- 
   -->
  <xs:complexType name="XWithCard">
    <xs:complexContent>
      <xs:restriction base="XCardBase">
        <xs:group ref="subSingle_g" minOccurs="0"/>
        <xs:attribute name="source" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSection">
    <xs:complexContent>
      <xs:restriction base="XSectionBase">
        <xs:group ref="subSingle_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionRoot">
    <xs:complexContent>
      <xs:restriction base="XSectionBase">
        <xs:group ref="subRoot_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  No pane (no Card) from Card-dialog
	-->
  <xs:complexType name="XHiddenBaseBase">
    <xs:complexContent>
      <xs:extension base="XInvisibleDialogComponentBase">
        <xs:attribute name="pluginId" type="PluginIdType" default="com.maconomy.client.pane.card"/>
        <xs:attribute name="type" type="XeComponentType" fixed="Hidden"/>
        <xs:attribute name="hidden" type="XHiddenType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="XHiddenBase">
    <xs:complexContent>
      <xs:restriction base="XHiddenBaseBase">
        <xs:group ref="subHidden_g" minOccurs="0"/>
        <xs:attribute name="title" use="prohibited" />
        <xs:attribute name="layout" use="prohibited" />
        <xs:attribute name="view" use="prohibited" />
        <xs:attribute name="type" use="prohibited"/>
        <xs:attribute name="hidden" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XHidden">
    <xs:complexContent>
      <xs:restriction base="XHiddenBase">
        <xs:group ref="subHidden_g" minOccurs="0"/>
        <xs:attribute name="source" type="IdType" use="required"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XWithHidden">
    <xs:complexContent>
      <xs:restriction base="XHiddenBase">
        <xs:group ref="subHidden_g" minOccurs="0"/>
        <xs:attribute name="source" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionBaseBase">
    <xs:complexContent>
      <xs:extension base="XPaneBase">
        <xs:attribute name="pluginId" type="PluginIdType" default="com.maconomy.client.pane.empty"/>  
        <xs:attribute name="type" type="XeComponentType" fixed="Section"/>
        <xs:attribute name="hidden" type="XHiddenType" />
      </xs:extension>      
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionBase">
    <xs:complexContent>
      <xs:restriction base="XSectionBaseBase">
        <xs:group ref="subBase_g" minOccurs="0" />
        <xs:attribute name="title" type="TitleType" use="required"/>
        <xs:attribute name="name" type="IdType" />
        <xs:attribute name="source" use="prohibited" />        
        <xs:attribute name="view" use="prohibited" />        
        <xs:attribute name="layout" use="prohibited" />
        <xs:attribute name="type" use="prohibited"/>        
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionSingle">
    <xs:complexContent>
      <xs:restriction base="XSectionBase">
        <xs:group ref="subSectionSingle_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionTable">
    <xs:complexContent>
      <xs:restriction base="XSectionBase">
        <xs:group ref="subSectionTable_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XSectionFilter">
    <xs:complexContent>
      <xs:restriction base="XSectionBase">
        <xs:group ref="subSectionFilter_g" minOccurs="0"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XWorkspaceBase">
    <xs:complexContent>
      <xs:extension base="XComponentBase">
        <xs:attribute name="type" type="XeComponentType" fixed="Root"/>      
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->  
  <xs:complexType name="XWorkspace">
    <xs:complexContent>
     <xs:restriction base="XWorkspaceBase">
        <xs:group ref="topRoot_g" minOccurs="0"/>
        <xs:attribute name="type" use="prohibited"/>
        <xs:attribute name="name" type="IdType" use="required"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
    Extends ComponentBase by Workspace-attrs
  -->
  <xs:complexType name="XWorkspaceRefBase">
    <xs:complexContent>
      <xs:extension base="XComponentBase">
        <xs:attribute name="source" type="IdType" use="required"/>
        <xs:attribute name="type" type="XeComponentType" fixed="Reference"/>      
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
    Restrict/deny inner tags in Workspace-tags
  -->
  <xs:complexType name="XWorkspaceRef">
    <xs:complexContent>
      <xs:restriction base="XWorkspaceRefBase">
        <xs:attribute name="title" use="prohibited"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>  
    </xs:complexContent>
  </xs:complexType>
  <!--
  Abstract generic Compositions
	-->
  <xs:complexType name="XClumpBase" abstract="true">
    <xs:annotation><xs:appinfo>
        <ai:interfaces check="false">jaxb.mwsl.structure.XiClump</ai:interfaces>
    </xs:appinfo></xs:annotation>  
    <xs:attribute name="minimized" type="BooleanType" />
    <xs:attribute name="horizontal" type="BooleanType" />
    <xs:attribute name="name" type="IdType" />
    <xs:attribute name="hidden" type="XHiddenType" default="false" />
    <xs:attribute name="parentSize" type="XeSizeType" default="standard" />
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XCompositionsBase" abstract="true">
    <xs:complexContent>
      <xs:extension base="XClumpBase">
        <xs:group ref="bindsBase_g" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XExpansionsBase">
    <xs:annotation><xs:appinfo>
        <ai:interfaces check="false">jaxb.mwsl.structure.XiExpansions</ai:interfaces>
    </xs:appinfo></xs:annotation>
    <xs:complexContent>
      <xs:extension base="XCompositionsBase">
        <xs:attribute name="type" type="XeClumpType" fixed="Expansions" /> 
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XExpansionsFilter">
    <xs:complexContent>
      <xs:restriction base="XExpansionsBase">
        <xs:group ref="bindsFilter_g"/>
        <xs:attribute name="horizontal" type="BooleanType" default="false"/>
        <xs:attribute name="minimized" type="BooleanType" default="false"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XExpansionsSingle">
    <xs:complexContent>
      <xs:restriction base="XExpansionsBase">
        <xs:group ref="bindsSingle_g"/>
        <xs:attribute name="horizontal" type="BooleanType" default="false"/>
        <xs:attribute name="minimized" type="BooleanType" default="false"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
	-->
  <xs:complexType name="XExpansionsSectionBase">
    <xs:complexContent>
      <xs:restriction base="XExpansionsBase">
        <xs:group ref="bindsBase_g"/>
        <xs:attribute name="type" use="prohibited"/>
        <xs:attribute name="horizontal" use="prohibited" />
        <xs:attribute name="hidden" use="prohibited"/>
        <xs:attribute name="parentSize" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XExpansionsSubRoot">
    <xs:complexContent>
      <xs:restriction base="XExpansionsSectionBase">
        <xs:group ref="bindsSubRoot_g"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>

  <!--
  -->
  <xs:complexType name="XExpansionsSectionFilter">
    <xs:complexContent>
      <xs:restriction base="XExpansionsSectionBase">
        <xs:group ref="bindsFilter_g"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XExpansionsSectionSingle">
    <xs:complexContent>
      <xs:restriction base="XExpansionsSectionBase">
        <xs:group ref="bindsSingle_g"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XExpansionsSectionTable">
    <xs:complexContent>
      <xs:restriction base="XExpansionsSectionBase">
        <xs:group ref="bindsSingle_g"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XFormationBaseBase" abstract="true">
    <xs:complexContent>
      <xs:restriction base="XClumpBase">
        <xs:attribute name="horizontal" use="prohibited" />
        <xs:attribute name="parentSize" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
  -->
  <xs:complexType name="XFormationBase">
    <xs:annotation><xs:appinfo>
        <ai:interfaces check="false">jaxb.mwsl.structure.XiExpansions</ai:interfaces>
    </xs:appinfo></xs:annotation>
    <xs:complexContent>
      <xs:extension base="XFormationBaseBase">
        <xs:group ref="sectionsBase_g" />
        <xs:attribute name="type" type="XeClumpType" fixed="Formation"/>    
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="XFormationPostBase">
    <xs:complexContent>
      <xs:restriction base="XFormationBase">
          <xs:group ref="sectionsBase_g"/>
        <xs:attribute name="type" use="prohibited"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>  
  <!--
    Formation used direct inside a Filter-tag
  -->
  <xs:complexType name="XFormationFilter">
    <xs:complexContent>
      <xs:restriction base="XFormationPostBase">
         <xs:group ref="sectionsFilter_g"  />
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
   -->
  <xs:complexType name="XFormationSectionFilter">
    <xs:complexContent>
      <xs:restriction base="XFormationPostBase">
        <xs:group ref="sectionsFilter_g"  />
        <xs:attribute name="minimized" use="prohibited" />
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
    Formation used direct inside a Card/PrintCard-tag
  -->
  <xs:complexType name="XFormationSingle">
    <xs:complexContent>
      <xs:restriction base="XFormationPostBase">
        <xs:group ref="sectionsSingle_g"/>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <!--
   -->
  <xs:complexType name="XFormationSectionSingle">
    <xs:complexContent>
      <xs:restriction base="XFormationPostBase">
